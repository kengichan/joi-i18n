{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "moduleResolution": "node",
    "allowJs": true,
    "downlevelIteration": true,
    "removeComments": true,
    "sourceMap": false,
    "alwaysStrict": true,
    "outDir": "dist"
  },
  "include": [
    "src/*.ts",
    "test/*.ts",
    "types.d.ts"
  ]
}
